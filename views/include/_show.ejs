<% for(var i = 0; i<users.length; i++){ %> 
<tr class="table_row">
    <td><%= i+1 %> </td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].status %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id %>" class="btn main_btn border-shadow update box-nav">
            <span class="text-gradient">
                <i class="fa-solid fa-pencil"></i>
            </span>
        </a>
        <a class="btn main_btn border-shadow delete box-nav" data-id=<%= users[i]._id %>>
            <span class="text-gradient">
                <i class="fa-solid fa-times"></i>
            </span>
        </a>
    </td>
</tr>


<% } %> 